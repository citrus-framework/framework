{
  "name": "citrus-framework/framework",
  "description": "php web framework",
  "keywords": ["citrus", "php", "framework"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/citrus-framework/framework",
  "authors": [
    {
      "name": "take64",
      "email": "take64@citrus.tk"
    }
  ],
  "require": {
    "citrus-framework/configure": "^1.0",
    "citrus-framework/contract": "^1.0",
    "citrus-framework/http": "^1.0",
    "citrus-framework/logger": "^1.0",
    "citrus-framework/sqlmap": "^1.0",
    "citrus-framework/variables": "^1.1",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-memcached": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-posix": "*",
    "ext-redis": "*"
  },
  "require-dev": {
    "php": "^8.1",
    "phpunit/phpunit": "^9.5",
    "nunomaduro/phpinsights": "^v2.0"
  },
  "autoload": {
    "psr-4": {
      "Citrus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
